{
  "name": "pride-icons",
  "version": "0.1.0",
  "repository": "git@github.com:lolPants/pride-icons.git",
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "check": "tsc --noEmit",
    "test": "yarn run check && yarn run lint",
    "dev": "parcel ./src/index.html",
    "clean": "rimraf .cache dist",
    "build": "parcel build ./src/index.html",
    "build:prod": "yarn run clean && parcel build --public-url . --no-source-maps --experimental-scope-hoisting ./src/index.html"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@types/parcel-env": "^0.0.0",
    "autoprefixer": "^9.6.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-compress": "^1.0.10",
    "parcel-plugin-typed-css-modules": "^1.1.0",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "eventemitter3": "^4.0.0"
  }
}
