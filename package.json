{
  "name": "pride-icons",
  "version": "0.1.0",
  "repository": "git@github.com:lolPants/pride-icons.git",
  "author": "Jack Baron <jackmbaron@gmail.com> (https://www.jackbaron.com)",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "lint": "xo",
    "check": "tsc --noEmit",
    "test": "yarn run check && yarn run lint",
    "dev": "parcel ./src/index.html",
    "clean": "rimraf .cache dist",
    "build": "parcel build ./src/index.html",
    "build:prod": "yarn run clean && parcel build --no-source-maps --experimental-scope-hoisting ./src/index.html"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/runtime": "^7.14.0",
    "@types/node": "^12.0.0",
    "@types/parcel-env": "^0.0.0",
    "eslint-config-xo-typescript": "^0.41.1",
    "eslint-plugin-prettier": "^3.4.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-css-modules-type-generator": "^1.2.0",
    "parcel-plugin-subresource-integrity": "^0.3.4",
    "postcss-modules": "^3.2.2",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.8",
    "typescript": "^4.2.4",
    "xo": "^0.39.1"
  },
  "dependencies": {
    "autoprefixer": "^9.8.6",
    "eventemitter3": "^4.0.7"
  }
}
